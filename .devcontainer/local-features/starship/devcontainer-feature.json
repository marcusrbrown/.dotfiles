{
    "id": "starship",
    "name": "Starship",
    "version": "none",
    "customizations": {
        "vscode": {
            "extensions": ["tamasfe.even-better-toml"],
            "settings": {
                "evenBetterToml.schema.associations": {
                    "**/starship.toml": "https://starship.rs/config-schema.json"
                }
            }
        }
    },
    "installsAfter": ["ghcr.io/devcontainers/features/common-utils"],
    "options": {
        "version": {
            "type": "string",
            "proposals": ["latest"],
            "default": "latest",
            "description": "Select the version of Starship to install."
        }
    }
}
