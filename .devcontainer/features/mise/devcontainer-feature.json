{
    "id": "mise",
    "name": "Mise",
    "version": "none",
    "dependsOn": {
        "./features/dotfiles-dev": {}
    },
    "installsAfter": ["ghcr.io/devcontainers/features/common-utils"],
    "customizations": {
        "vscode": {
            "extensions": ["hverlin.mise-vscode"]
        }
    },
    "postCreateCommand": "eval \"$(mise activate $(basename $SHELL))\"; mise install; mise doctor"
}
